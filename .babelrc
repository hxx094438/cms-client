{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2",
    "es2015", 
    "stage-0", 
    "react"
  ],
  "plugins": [
    "transform-vue-jsx",
     "transform-runtime",
     "transform-decorators-legacy",
     "transform-class-properties"
    ]
}
